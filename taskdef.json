{
    "executionRoleArn": "arn:aws:iam::495599755529:role/ecsCodeDeployRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 5000,
            "protocol": "tcp",
            "containerPort": 5000
          }
        ],

        "image": "imageDetail.json",
        "essential": true,
        "name": "Container-app-python"
      }
